    <dependency>
      <groupId>thulac.lite</groupId>
      <artifactId>thulac.lite</artifactId>
      <version>0.0.1</version>
      <scope>system</scope>
      <systemPath>E:\eclipse-workspace\AutoLabel\THULAC_lite_java_run.jar</systemPath>
    </dependency>
    <!-- https://mvnrepository.com/artifact/com.hankcs/hanlp -->
    <dependency>
      <groupId>com.hankcs</groupId>
      <artifactId>hanlp</artifactId>
      <version>portable-1.5.2</version>
